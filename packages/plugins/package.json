{
  "name": "@editablejs/plugins",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": true,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "test": "jest",
    "build": "tsup & postcss ./src/styles.css --dir dist",
    "dev": "concurrently \"tsup --watch\" \"postcss ./src/styles.css --dir dist --watch\"",
    "lint": "eslint src --fix",
    "format": "prettier --write",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@editablejs/editor": "workspace:*",
    "@types/escape-html": "^1.0.2",
    "@types/react": "^17.x",
    "@types/react-dom": "^17.x",
    "autoprefixer": "10.4.8",
    "concurrently": "7.4.0",
    "cssnano": "5.1.13",
    "cssnano-preset-advanced": "5.3.8",
    "eslint-config-editable": "workspace:*",
    "jest": "^27.5.1",
    "jest-presets": "workspace:*",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "15.0.0",
    "prettier-config": "workspace:*",
    "react": "^17.x",
    "react-dom": "^17.x",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.1.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.2",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "@editablejs/editor": "workspace:*"
  },
  "lint-staged": {
    "src/**/*.{json,ts,tsx,md,mdx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "1.0.0",
    "@radix-ui/react-dropdown-menu": "1.0.0",
    "@radix-ui/react-tooltip": "1.0.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "3.3.2",
    "escape-html": "^1.0.3",
    "slate-hyperscript": "^0.77.0"
  }
}
